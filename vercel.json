{
  "version": 2,
  "builds": [
    {
      "src": "api/endpoints/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/nome",
      "dest": "/api/endpoints/nome.js"
    },
    {
      "src": "/telefone",
      "dest": "/api/endpoints/telefone.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
